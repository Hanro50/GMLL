{
    "name": "gmll",
    "version": "0.0.1",
    "description": "A generic launcher core for building custom launchers",
    "license": "MIT",
    "exports": {
        ".": {
            "import": "./dist/mjs/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "main": "dist/cjs/index.js",
    "module": "dist/mjs/index.js",
    "dependencies": {
        "7zip-min": "^1.4.0",
        "node-fetch": "^2.6.6",
        "tslib": "^2.3.1"
    },
    "devDependencies": {
        "@types/node": "^16.11.10",
        "node": "^14.18.1"
    },
    "types": "/dist/types/index.d.ts",
    "files": [
        "/dist/**/*",
        "/dist/**/**/*",
        "/package.json",
        "/LICENSE",
        "/README.md"
    ],
    "engines": {
        "node": ">=14"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Hanro50/GMLL"
    },
    "funding": [
        {
            "type": "paypal",
            "url": "https://paypal.me/hanro50"
        }
    ],
    "scripts": {
        "build": "node build.cjs"
    }
}